<%- model_class = Event -%>
<%- model_class_et = EventTranslation -%>
<% title t '.title', :default => model_class.model_name.human %>

<div class="form-actions">
  <%= link_to t('helpers.links.list'),
              admin_events_path, :class => 'btn'  %>
  <%= link_to t('helpers.links.edit'),
              edit_admin_event_path(@event), :class => 'btn' %>
  <%= link_to t('helpers.links.delete'),
              admin_user_path(@event),
              :method => :delete,
							:data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
</div>

<div id="timeline-embed"></div>


